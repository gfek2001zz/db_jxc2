<?xml version="1.0" encoding="UTF-8" ?>
<excel name="goodsExport" rows="100">
    <sheet name="床垫" titleRow="1" dataRow="2" providerBean="mattressExportProvider">
        <column fieldName="brand" type="string" width="20">品牌</column>
        <column fieldName="model" type="string" width="20">型号</column>
        <column fieldName="size" type="string" width="20">尺寸(CM)</column>
        <column fieldName="sellingPrice" width="20">零售价</column>
        <column fieldName="purchasingPrice" width="20">采购价</column>
    </sheet>

    <sheet name="皮床" titleRow="1" dataRow="2" providerBean="bedExportProvider">
        <column fieldName="brand" type="string" width="20">品牌</column>
        <column fieldName="model" type="string" width="20">型号</column>
        <column fieldName="name" type="string" width="20">品名</column>
        <column fieldName="style" type="string" width="20">款式</column>
        <column fieldName="color" type="string" width="20">颜色</column>
        <column fieldName="size" type="string" width="20">尺寸(CM)</column>
        <column fieldName="sellingPrice" type="double" width="20">零售价</column>
        <column fieldName="purchasingPrice" type="double" width="20">采购价</column>
    </sheet>
</excel>