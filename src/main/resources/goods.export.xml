<?xml version="1.0" encoding="UTF-8" ?>
<excel name="goodsExport" rows="100">
    <sheet name="床垫" consumerBean="mattressExportConsumer">
        <column fieldName="brand" type="string">品牌</column>
        <column fieldName="model" type="string">型号</column>
        <column fieldName="size" type="string">尺寸(CM)</column>
        <column fieldName="sellingPrice">零售价</column>
        <column fieldName="purchasingPrice">采购价</column>
    </sheet>

    <sheet name="皮床" consumerBean="bedExportConsumer">
        <column fieldName="brand" type="string">品牌</column>
        <column fieldName="model" type="string">型号</column>
        <column fieldName="style" type="string">款式</column>
        <column fieldName="color" type="string">颜色</column>
        <column fieldName="size" type="string">尺寸(CM)</column>
        <column fieldName="sellingPrice" type="double">零售价</column>
        <column fieldName="purchasingPrice" type="double">采购价</column>
    </sheet>
</excel>